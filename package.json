{
  "name": "venkatasudha",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf dist coverage cypress tmp && yarn install",
    "validate:commit": "run-s clean test:** reports:coverage e2e:**",
    "validate:all": "run-s validate:commit open:portfolio-client serve:portfolio-client",
    "build:all": "nx affected --target=build --parallel",
    "lint:all": "nx affected --target=lint --parallel",
    "e2e:all": "nx affected --target=e2e --parallel --ci=true",
    "test:all": "nx affected --target=test --parallel --coverage",
    "storybook:all": "nx affected --target=storybook --all --parallel",
    "serve:portfolio": "run-s serve:portfolio-client",
    "serve:portfolio-client": "nx run portfolio-client:build:production && nx run portfolio-client:serve:production",
    "reports:coverage": "node ./tools/scripts/coverageMerger.mjs",
    "open:portfolio-client": "open-cli http://localhost:4200",
    "upgrade:nx": "nx migrate latest && nx migrate --run-migrations && rimraf migrations.json",
    "prepare": "husky install"
  },
  "private": true,
  "dependencies": {
